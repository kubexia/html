{
    "name":"kubexia/kubexia",
    "description":"Laravel Components",
    "type":"library",
    "license": "MIT",
    "require": {
        "php": ">=5.5.9",
        "rcrowe/twigbridge": "^0.9.2"
    },
    "autoload": {
        "psr-4": {
            "Kubexia\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "scripts": {
        "post-install-cmd": [
            "rm app/User.php",
            "rm app/Http/Controller.php",
